{
    "name": "olat-nji/midone-admin",
    "keywords": [
        "laravel",
        "auth"
    ],
    "license": "MIT",
    "description": "A Midone powered, laravel built admin dashboard, fully customized with an auth capabilities",
    "authors": [{
        "name": "Olatunji Olayemi",
        "email": "olayemi289@gmail.com"
    }],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "jenssegers/agent": "^2.6",
        "laravel/fortify": "^1.6.1",
        "doctrine/dbal": "^2.12",
        "livewire/livewire": "^2.3"
    },
    "require-dev": {

        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"

    },
    "autoload": {
        "psr-4": {
            "Olatunji\\MidoneAdmin\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "stubs/app/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Olatunji\\MidoneAdmin\\MainServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
